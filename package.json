{
  "name": "real-react-google-multi-lang",
  "version": "1.2.0",
  "description": "A library to support multi-language translation in React apps using Google Translation API.",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "NODE_ENV=production babel src --out-dir dist",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "prettify": "prettier --write 'src/**/*.{js,jsx,ts,tsx,json,css,md}'",
    "prepare": "npm run build",
    "prepublishOnly": "npm run prettify && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SamuelCody/real-react-google-multi-lang.git"
  },
  "bugs": {
    "url": "https://github.com/SamuelCody/real-react-google-multi-lang/issues"
  },
  "homepage": "https://github.com/SamuelCody/real-react-google-multi-lang#readme",
  "keywords": [
    "react",
    "translation",
    "google-translate",
    "multi-language",
    "javascript"
  ],
  "author": "Samuel Ayinde",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.10.0",
    "dotenv": "^17.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.0",
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.31.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "babel-preset-minify": "^0.5.2",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react": "^7.37.5",
    "prettier": "^3.6.2"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  }
}
